  dialogue:
  - dialogueText: He steps closer, trembling.
    characterName: Narrator
    characterImage: {fileID: 0}
    dialogueEnd: 0
  - dialogueText: "You\u2019d\u2026 let me?"
    characterName: Vampire
    characterImage: {fileID: 21300000, guid: b0c562d4617610b4286133476bb79152, type: 3}
    dialogueEnd: 0
  - dialogueText: He leans forward, you feel his breath on your neck.
    characterName: Narrator
    characterImage: {fileID: 0}
    dialogueEnd: 0
  - dialogueText: He hesitates before you feel his fangs sinking into your flesh,
